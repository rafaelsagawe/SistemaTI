@model IEnumerable<SistemaTI.Models.Equipamento>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.NuSerie)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NuPatrimonio)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.EquipValor)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IP)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Situacao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DataMovimantacao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Local)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Especificacao)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Processo)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NuSerie)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NuPatrimonio)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EquipValor)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IP)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Situacao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataMovimantacao)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Local.ID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Especificacao.Fabicante)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Processo.ProcessoId)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdEquipamento">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdEquipamento">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdEquipamento">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
