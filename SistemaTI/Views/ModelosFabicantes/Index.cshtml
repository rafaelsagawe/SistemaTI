@model IEnumerable<SistemaTI.Models.ModeloFabicante>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Descricao)
            </th>

            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Descricao)
            </td>

            <td>
                <a asp-action="Edit" asp-route-id="@item.idModelo">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.idModelo">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.idModelo">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
