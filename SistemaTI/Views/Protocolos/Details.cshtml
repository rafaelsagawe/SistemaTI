@model SistemaTI.Models.Protocolo

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Protocolo</h1>

<div>
    <h4>Protocolo</h4>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Processo)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Processo)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Assunto)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Assunto)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Tipo)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Tipo)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Prazo)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Prazo)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DataEntrada)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DataEntrada)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.DataRegistro)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.DataRegistro)
        </dd>
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Renovacao)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Renovacao)
        </dd>
       
        <dt class="col-sm-2">
            Movimentação
        </dt>

        <dd class="col-sm-10">
            <table class="table table-striped">
                <tr>
                    <th>Data</th>
                    <th>Localização</th>
                    <th>Usuário que tramitou</th>
                </tr>
                @foreach (var item in Model.Tramitar)
            {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem =>item.Movimentacao)
                        </td>

                        <td>
                            @Html.DisplayFor(modelItem => item.Localizacao)
                        </td>

                        <td>
                            @Html.DisplayFor(modelItem => item.UsuarioTramitacao)
                        </td>
                    </tr>
            }

            </table>
        </dd>

    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model.Id">Edit</a> |
    <a asp-action="Index">Back to List</a>
</div>
